{
    "name": "codezero/laravel-localized-routes",
    "description": "A convenient way to set up, manage and use localized routes in a Laravel app.",
    "keywords": [
        "php",
        "laravel",
        "localization",
        "locale",
        "language",
        "country",
        "routes",
        "routing",
        "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Vermeyen",
            "email": "ivan@codezero.be"
        }
    ],
    "require": {
        "php": "^7.1",
        "0.0.0/composer-include-files": "^1.5",
        "illuminate/support": "^5.6|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*",
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "CodeZero\\LocalizedRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeZero\\LocalizedRoutes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeZero\\LocalizedRoutes\\LocalizedRoutesServiceProvider"
            ]
        },
        "include_files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
