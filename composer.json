{
    "name": "codezero/laravel-localized-routes",
    "description": "A convenient way to set up, manage and use localized routes in a Laravel app.",
    "keywords": [
        "php",
        "laravel",
        "localization",
        "locale",
        "language",
        "country",
        "routes",
        "routing",
        "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Vermeyen",
            "email": "ivan@codezero.be"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "0.0.0/composer-include-files": "^1.5",
        "codezero/browser-locale": "^3.0",
        "codezero/laravel-uri-translator": "^1.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "CodeZero\\LocalizedRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeZero\\LocalizedRoutes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeZero\\LocalizedRoutes\\LocalizedRoutesServiceProvider"
            ]
        },
        "include_files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "0.0.0/composer-include-files": true,
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
